	xsi:schemaLocation="http://www.airbus.com/topcased/gPM">
	<version>1.2</version><!-- Dictionnary's description  -->
	<dictionary>
		<!-- Categories's description  -->
		<categories>
			<!-- Color Category  -->
			<category name="Color">
				<values>
