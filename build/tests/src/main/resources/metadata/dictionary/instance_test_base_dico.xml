			<category name="Usage">
				<values>
					<categoryValue value="Self Defense" />
					<categoryValue value="Police" />
					<categoryValue value="Children" />
				</values>
			</category>

			<!-- Category modifiable by any user -->
			<category name="ErrorLevel" access="User">
				<values>
					<categoryValue value="Fatal" />
					<categoryValue value="Severe" />
