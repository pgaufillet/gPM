<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition
  xmlns="http://jbpm.org/3/jpdl"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://jbpm.org/3/jpdl http://jbpm.org/xsd/jpdl-3.0.xsd"
  name="Analyse">
   <start-state name="En cours">
      <transition name="Résoudre" to="Résolu"></transition>
      <transition name="Contourner" to="Contourné"></transition>
   </start-state>
   <state name="Résolu">
      <transition name="Livrer" to="Livré"/>
   </state>
   <state name="Contourné">
      <transition name="Livrer" to="Livré"/>
   </state>
   <end-state name="Livré"></end-state>
</process-definition>