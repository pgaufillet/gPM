<?xml version="1.0" encoding="ISO-8859-1"?>
<gpm xmlns="http://www.airbus.com/topcased/gPM" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.airbus.com/topcased/gPM">
	<version>1.2</version>
	<filters>
		<sheetFilter labelKey="Cat00NumberWithStore">
			<containers>
				<sheetTypeRef name="Cat" />
			</containers>
			<criteria>
				<criteriaGroup>
					<criterion fieldKey="$PRODUCT_NAME" operator="=" value="product4"
						caseSensitive="true" />
				</criteriaGroup>
			</criteria>
			<resultSummary>
				<fieldResult name="$SHEET_REFERENCE" sort="asc"
					displayed="true" label="Reference Sheet" />
				<fieldResult name="$PRODUCT_NAME" sort="asc" displayed="true"
					label="Produit associe" />
				<fieldResult name="Cat-Cat|Cat|$SHEET_REFERENCE" sort="asc" displayed="true"
					label="Reference Sheet Link" />
				<fieldResult name="Cat-Cat|Cat|Store|product_name" sort="asc" displayed="true"
                    label="Store Name" />
			</resultSummary>
		</sheetFilter>
		
		<sheetFilter labelKey="Cat00NumberWithoutStore">
            <containers>
                <sheetTypeRef name="Cat" />
            </containers>
            <criteria>
                <criteriaGroup>
                    <criterion fieldKey="$PRODUCT_NAME" operator="=" value="product4"
                        caseSensitive="true" />
                </criteriaGroup>
            </criteria>
            <resultSummary>
                <fieldResult name="$SHEET_REFERENCE" sort="asc"
                    displayed="true" label="Reference Sheet" />
                <fieldResult name="$PRODUCT_NAME" sort="asc" displayed="true"
                    label="Produit associe" />
                <fieldResult name="Cat-Cat|Cat|$SHEET_REFERENCE" sort="asc" displayed="true"
                    label="Reference Sheet Link" />
            </resultSummary>
        </sheetFilter>
	</filters>
</gpm>