        <profile>
            <id>gPM.db.disabled</id>
            <activation>
                <property>
                    <name>!gPM.db</name>
                </property>
            </activation>
            <properties>
                <!-- Disable the unit tests if no database support selected -->
                <maven.test.skip>true</maven.test.skip>
            </properties>
        </profile>

        <profile>
            <id>gPM.tests.disabled</id>
            <activation>
                <property>
                    <name>gPM.tests</name>
                    <value>false</value>
                </property>
            </activation>
            <properties>
