            <properties>
                <gpm.integration-tests>
                    **/IntegrationSuite.java
                </gpm.integration-tests>
            </properties>
        </profile>
        <profile>
            <id>gPM.it.disabled</id>
            <activation>
                <property>
                    <name>!gPM.it</name>
                </property>
            </activation>
            <properties>
                <!-- Disable the integration tests if no property specified -->
                <gpm.integration />
            </properties>
        </profile>

        <profile>
            <id>gPM.ut.enabled</id>
            <activation>
                <property>
                    <name>!gPM.ut</name>
                </property>
