                        <reports>
                            <!-- project dependencies -->
                            <report>dependencies</report>
                        </reports>
                    </reportSet>
                </reportSets>
            </plugin>

            <!-- Project source code -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jxr-plugin</artifactId>
            </plugin>

            <!--
                Standard PMD checks : * basic rule : empty blocks,
                unecessary statements and possible bugs * unused code :
                unused private fields, methods, variables and parameters
                * imports : duplicate, redundant or unused import
                declarations
            -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-pmd-plugin</artifactId>

                <configuration>
                    <targetJdk>1.6</targetJdk>
                </configuration>
            </plugin>

            <!-- CheckStyle -->

        </plugins>
    </reporting>

