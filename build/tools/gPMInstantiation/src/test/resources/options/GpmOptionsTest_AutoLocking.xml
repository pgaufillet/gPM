<?xml version="1.0" encoding="UTF-8"?>
<gpm xmlns="http://www.airbus.com/topcased/gPM" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.airbus.com/topcased/gPM http://www.airbus.com/topcased/gPM ">
	<version>1.2</version>

	<options>
		<autolocking>WRITE</autolocking>
	</options>

	<sheetTypes>
		<sheetType name="Autolocking_Sheet" autolocking="READ_WRITE">
			<referenceField>
				<fields>
					<simpleField valueType="String" labelKey="ref" />
				</fields>
			</referenceField>
			<fields>
				<simpleField valueType="String" labelKey="field" />
			</fields>

			<lifecycleResource>
            options/lifecycle/autolocking_Sheet.par/processdefinition.xml
        </lifecycleResource>
		</sheetType>
	</sheetTypes>
</gpm>